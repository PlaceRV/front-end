{
	"name": "tmpl-frontend",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"backend": "npm i git+ssh://github.com/PlaceRV/back-end#package --save-dev",
		"build": "ng build",
		"format": "replace '{\n' '{' . -r --include=\"*.ts,*.js*\" --exclude=\"node_modules\" --silent && replace \"\\[\n\" \"[\" . -r --include=\"*.ts,*.js*\" --exclude=\"node_modules\" --silent && prettier --write \"./**/*.{html,ts,css,scss}\" \"./**/*.js*\" \"./**/*.*rc\" --log-level silent",
		"lint": "ng lint --fix",
		"ng": "ng",
		"start": "ng serve",
		"watch": "ng build --watch --configuration development"
	},
	"dependencies": {
		"@angular-devkit/build-angular": "^18.2.0",
		"@angular/animations": "^18.1.0",
		"@angular/cdk": "^18.1.2",
		"@angular/common": "^18.1.0",
		"@angular/compiler": "^18.1.0",
		"@angular/core": "^18.1.0",
		"@angular/forms": "^18.1.0",
		"@angular/material": "^18.1.2",
		"@angular/platform-browser": "^18.1.0",
		"@angular/platform-browser-dynamic": "^18.1.0",
		"@angular/router": "^18.1.0",
		"@apollo/client": "^3.11.8",
		"apollo-angular": "^7.1.2",
		"gh-pages": "^6.1.1",
		"graphql": "^16.9.0",
		"ol": "^10.0.0",
		"rxjs": "~7.8.0",
		"zone.js": "~0.14.3"
	},
	"devDependencies": {
		"@angular/cli": "^18.1.2",
		"@ianvs/prettier-plugin-sort-imports": "^4.3.1",
		"@types/jasmine": "~5.1.0",
		"angular-eslint": "18.3.0",
		"daisyui": "^4.12.10",
		"eslint": "^9.9.0",
		"place-review-types": "github:PlaceRV/back-end#package",
		"postcss": "^8.4.40",
		"prettier": "^3.3.3",
		"prettier-plugin-sort-json": "^4.0.0",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"replace": "^1.2.2",
		"tailwindcss": "^3.4.7",
		"tslib": "^2.7.0",
		"typescript": "~5.5.2",
		"typescript-eslint": "8.3.0"
	}
}
